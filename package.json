{
  "name": "dp05-react",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .jsx,.js,.json",
    "lint:fix": "eslint src --ext .jsx,.js,.json --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "prettier --check . && eslint src --ext .jsx,.js,.json",
    "format:fix": "prettier --write . && eslint src --ext .jsx,.js,.json --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "antd": "^5.4.0",
    "axios": "^1.3.5",
    "babel-eslint": "^10.1.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cors-anywhere": "^0.4.4",
    "formik": "^2.2.8",
    "json-server": "^0.16.3",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-toastify": "^9.1.2",
    "react-with-firebase-auth": "^1.4.1",
    "redux": "^4.1.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "commitlint": "^16.0.1",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-import-order-alphabetical": "^1.0.1",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^8.0.1",
    "prettier": "^2.5.1",
    "vite": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(jsx|js|json|css|html)": [
      "precheck"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
